{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "messageId": {
      "type": "string",
      "format": "uuid",
      "description": "Unique identifier for idempotency"
    },
    "wellId": {
      "type": "string",
      "minLength": 1,
      "description": "Well ID"
    },
    "name": {
      "type": "string",
      "minLength": 1,
      "description": "Token name"
    },
    "symbol": {
      "type": "string",
      "minLength": 1,
      "description": "Token symbol"
    },
    "decimals": {
      "type": "integer",
      "minimum": 0,
      "maximum": 18,
      "description": "Token decimals"
    },
    "initialSupply": {
      "type": "string",
      "pattern": "^[0-9]+$",
      "description": "Initial supply as string"
    }
  },
  "required": ["messageId", "wellId", "name", "symbol", "decimals", "initialSupply"],
  "additionalProperties": false
}