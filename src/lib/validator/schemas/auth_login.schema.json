{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "messageId": {
      "type": "string",
      "format": "uuid",
      "description": "Unique identifier for idempotency"
    },
    "username": {
      "type": "string",
      "minLength": 1,
      "description": "Username"
    },
    "password": {
      "type": "string",
      "minLength": 1,
      "description": "Password"
    }
  },
  "required": ["messageId", "username", "password"],
  "additionalProperties": false
}